{
  "name": "@maplat/core",
  "version": "0.10.3",
  "description": "Maplat is the cool Historical Map/Illustrated Map Viewer API.\nIt can transform each map coordinates with nonlinear but homeomorphic projection and makes possible that the maps can collaborate with GPS/accurate maps, without distorting original maps.",
  "main": "es5/index.js",
  "module": "lib/index.js",
  "sideEffects": true,
  "bin": {
    "freezer": "bin/freezer.js"
  },
  "files": [
    "src/",
    "dist/",
    "parts/",
    "lib/",
    "es5/"
  ],
  "scripts": {
    "server": "webpack-cli serve --config ./webpack_config/webpack.config.standard.dev.js --mode=development --devtool=inline-source-map",
    "server:packed": "webpack-cli serve --config ./webpack_config/webpack.config.packed.dev.js  --mode=development --devtool=inline-source-map",
    "server:e2e": "webpack-cli serve --config ./webpack_config/webpack.config.e2e.js  --mode=development",
    "server:e2e:start": "gulp e2e:start",
    "server:e2e:stop": "gulp e2e:stop",
    "build": "npm run build:standard && npm run build:packed && npm run build:es6 && npm run build:es5",
    "build:standard": "webpack --color --progress --config ./webpack_config/webpack.config.standard.js",
    "build:packed": "webpack --config ./webpack_config/webpack.config.packed.js",
    "build:es6": "tsc --build ts_config/tsconfig.es6.json",
    "build:es5": "tsc --build ts_config/tsconfig.es5.json",
    "zip": "gulp zip",
    "test": "npm run test:functions",
    "test:functions": "jest --config jest_config/jest.config.functions.js",
    "test:puppeteer": "jest --config jest_config/jest.config.puppeteer.js",
    "test:e2e": "npm run server:e2e:start && npm run test:puppeteer && npm run server:e2e:stop",
    "preversion": "npm run lint & npm run test",
    "version": "npm run build && npm run zip && git add -A",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,spec}/**/*.{js,ts}\" --fix",
    "lint:prettier": "prettier \"./{src,spec}/**/*.{js,ts}\" --write",
    "typecheck": "tsc --noEmit --project ts_config/tsconfig.es6.json",
    "git_switch": "gulp git_switch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/MaplatCore.git"
  },
  "keywords": [
    "warp",
    "GIS"
  ],
  "author": "Kohei Otsuka, Code for History",
  "license": "Maplat Limited License 1.1",
  "bugs": {
    "url": "https://github.com/code4history/MaplatCore/issues"
  },
  "homepage": "https://github.com/code4history/MaplatCore/wiki",
  "dependencies": {
    "@maplat/tin": "^0.9.2",
    "@turf/line-intersect": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "argv": "^0.0.2",
    "i18next": "^21.8.4",
    "i18next-xhr-backend": "^3.2.2",
    "lodash.template": "^4.5.0",
    "ol": "^6.14.1",
    "weiwudi": "^0.1.4",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/eslint": "^8.4.2",
    "@types/estree": "^0.0.51",
    "@types/jest": "^27.5.1",
    "@types/lodash.template": "^4.5.1",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.22.7",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^26.3.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.5.2",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-zip": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "jest-cli": "^28.1.0",
    "jest-puppeteer": "^6.1.0",
    "less": "^4.1.2",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "psaux": "^0.4.0",
    "puppeteer": "^14.1.1",
    "sleep-promise": "^9.1.0",
    "terminate": "^2.5.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
