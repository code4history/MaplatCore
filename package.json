{
  "name": "@maplat/core",
  "version": "0.10.6",
  "description": "Maplat is the cool Historical Map/Illustrated Map Viewer API.\nIt can transform each map coordinates with nonlinear but homeomorphic projection and makes possible that the maps can collaborate with GPS/accurate maps, without distorting original maps.",
  "main": "es5/index.js",
  "module": "lib/index.js",
  "sideEffects": true,
  "bin": {
    "freezer": "bin/freezer.js"
  },
  "files": [
    "src/",
    "dist/",
    "parts/",
    "lib/",
    "es5/"
  ],
  "scripts": {
    "server": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack-cli serve --config ./webpack_config/webpack.config.standard.dev.js --host=127.0.0.1 --mode=development --devtool=inline-source-map",
    "server:packed": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack-cli serve --config ./webpack_config/webpack.config.packed.dev.js --host=127.0.0.1 --mode=development --devtool=inline-source-map",
    "server:e2e": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack-cli serve --config ./webpack_config/webpack.config.e2e.js --host=127.0.0.1 --mode=development",
    "server:e2e:start": "gulp e2e:start",
    "server:e2e:stop": "gulp e2e:stop",
    "build": "npm run build:standard && npm run build:packed && npm run build:es6 && npm run build:es5",
    "build:standard": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --color --progress --config ./webpack_config/webpack.config.standard.js",
    "build:packed": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --config ./webpack_config/webpack.config.packed.js",
    "build:es6": "tsc --build ts_config/tsconfig.es6.json",
    "build:es5": "tsc --build ts_config/tsconfig.es5.json",
    "zip": "gulp zip",
    "test": "npm run test:functions",
    "test:functions": "jest --config jest_config/jest.config.functions.js",
    "test:puppeteer": "jest --config jest_config/jest.config.puppeteer.js",
    "test:e2e": "npm run server:e2e:start && npm run test:puppeteer && npm run server:e2e:stop",
    "preversion": "npm run lint & npm run test",
    "version": "npm run build && npm run zip && git add -A",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{src,spec}/**/*.{js,ts}\" --fix",
    "lint:prettier": "prettier \"./{src,spec}/**/*.{js,ts}\" --write",
    "typecheck": "tsc --noEmit --project ts_config/tsconfig.es6.json",
    "git_switch": "gulp git_switch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/MaplatCore.git"
  },
  "keywords": [
    "warp",
    "GIS"
  ],
  "author": "Kohei Otsuka, Code for History",
  "license": "Maplat Limited License 1.1",
  "bugs": {
    "url": "https://github.com/code4history/MaplatCore/issues"
  },
  "homepage": "https://github.com/code4history/MaplatCore/wiki",
  "dependencies": {
    "@maplat/tin": "^0.9.4",
    "@turf/line-intersect": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "@types/mapbox-gl": "^1.8.1",
    "argv": "^0.0.2",
    "caniuse-lite": "^1.0.30001591",
    "i18next": "^21.8.4",
    "i18next-xhr-backend": "^3.2.2",
    "lodash.template": "^4.5.0",
    "mapbox-gl": "^1.8.1",
    "ol": "^10.3.0",
    "ol-mapbox-style": "^12.3.5",
    "weiwudi": "^0.1.4",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.2",
    "@types/estree": "^0.0.51",
    "@types/lodash.template": "^4.5.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "core-js": "^3.22.7",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.3.0",
    "fs-extra": "^10.1.0",
    "less": "^4.2.1",
    "less-loader": "^11.0.0",
    "prettier": "^2.6.2",
    "psaux": "^0.4.0",
    "sleep-promise": "^9.1.0",
    "terminate": "^2.5.0",
    "terser": "^5.36.0",
    "typescript": "^4.7.2",
    "vite": "^5.4.11",
    "vite-plugin-banner": "^0.8.0",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.21.1"
  },
  "overrides": {
    "@babel/traverse": "7.23.2",
    "glob-parent": "5.1.2",
    "axios": "1.6.0",
    "minimist": "1.2.8"
  }
}
